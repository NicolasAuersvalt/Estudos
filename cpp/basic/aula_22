#include <iostream>

using namespace std;

void soma(int n1, int n2);

/* Prototipagem de função, ou seja, eu apresento a função primeiramente */
    /* para logo após executar se caso chamada. Tipo uma alocação na memória. */
    /* A prototipagem precisa ser igual à função normal!!!!! */ 

int main(){

    for(int i=0; i<10; i++){
        soma(15, 5);
    }

    /* for(condição){
    }
     */ 
    
    return 0;
}

void soma(int n1, int n2){
    cout << n1 + n2;
}

